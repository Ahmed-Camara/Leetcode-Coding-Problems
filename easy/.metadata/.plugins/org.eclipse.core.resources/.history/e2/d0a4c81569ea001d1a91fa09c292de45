
public class Main {
	
	public static void main(String[] args) {
		int myArr [] = {45, 55, 95, 105};
		System.out.println(removeDuplicates(myArr));
	}
	
	public static int removeDuplicates(int[] nums) {
        int count = 0;
        for(int i = 1; i < nums.length; i++){
            int j = i - 1;

            if(nums[i] == nums[j]){
                count++;
                nums[i] = -1;
                int k = i;

                while(k < nums.length){
                    int key = nums[k];
                    nums[k] = nums[k+1];
                    nums[k+1] = key;
                    k++;
                }
            }
        }

        return count;
    }
}
