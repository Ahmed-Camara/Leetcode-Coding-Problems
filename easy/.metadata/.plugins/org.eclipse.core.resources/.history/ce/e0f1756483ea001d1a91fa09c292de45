		int myArr [] = {0,0,1,1,1,2,2,3,3,4};
		System.out.println(removeDuplicates(myArr));
	}
	
	public static int removeDuplicates(int[] nums) {
        int count = 0;
        int[] newNums = new int[nums.length];
        
        for(int i = 0; i < nums.length; i++) {
        	
        	if(nums[i] != newNums[i]) {
        		newNums[i] = nums[i];
        	}
        }
       
        
