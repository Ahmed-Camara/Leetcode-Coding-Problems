	public static int removeDuplicates(int[] nums) {
		int count = 0;
		for(int i = 1; i < nums.length; i++) {
			int j = i - 1;
			
			if(nums[i] == nums[j]) {
				int temp = nums[i];
				nums[i] = nums[i + 1];
				nums[i + 1] = temp;
			}
		}
